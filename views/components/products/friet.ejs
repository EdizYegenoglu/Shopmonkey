<% friet.forEach(function(friet) { %>
    <article class="order col-sm-6 col-md-4 col-lg-4" data-id="<%= friet._id %>"
        data-product="<%= JSON.stringify(friet) %>">
        <img src="<%= friet.afbeelding %>" data-product-image>
        <h2 data-product-title>
            <%= friet.titel %>
        </h2>
        <p data-product-price>
            <%= friet.prijs %>
        </p>
    </article>
    <% }); %>