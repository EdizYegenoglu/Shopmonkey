<% broodjes.forEach(function(broodje) { %>
    <article class="order col-sm-6 col-md-4 col-lg-4" data-id="<%= broodje._id %>"
        data-product="<%= JSON.stringify(broodje) %>">
        <img src="<%= broodje.afbeelding %>" data-product-image>
        <h2 data-product-title>
            <%= broodje.titel %>
        </h2>
        <p data-product-price>
            <%= broodje.prijs %>
        </p>
    </article>
    <% }); %>